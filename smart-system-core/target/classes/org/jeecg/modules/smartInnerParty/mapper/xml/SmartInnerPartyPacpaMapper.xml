<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.smartInnerParty.mapper.SmartInnerPartyPacpaMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  smart_inner_party_pacpa 
		WHERE
			 inner_party_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.smartInnerParty.entity.SmartInnerPartyPacpa">
		SELECT * 
		FROM  smart_inner_party_pacpa
		WHERE
			 inner_party_id = #{mainId} 	</select>
</mapper>

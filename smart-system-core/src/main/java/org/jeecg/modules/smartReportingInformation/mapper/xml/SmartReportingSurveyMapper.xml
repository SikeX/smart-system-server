<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.smartReportingInformation.mapper.SmartReportingSurveyMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  smart_reporting_survey 
		WHERE
			 reporting_number = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.smartReportingInformation.entity.SmartReportingSurvey">
		SELECT * 
		FROM  smart_reporting_survey
		WHERE
			 reporting_number = #{mainId} 	</select>
</mapper>

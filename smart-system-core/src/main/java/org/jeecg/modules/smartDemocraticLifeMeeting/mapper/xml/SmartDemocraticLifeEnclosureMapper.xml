<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.smartDemocraticLifeMeeting.mapper.SmartDemocraticLifeEnclosureMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  smart_democratic_life_enclosure 
		WHERE
			 meeting_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.smartDemocraticLifeMeeting.entity.SmartDemocraticLifeEnclosure">
		SELECT * 
		FROM  smart_democratic_life_enclosure
		WHERE
			 meeting_id = #{mainId} 	</select>
</mapper>
